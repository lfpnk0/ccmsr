      <ul class="dropdown-menu pull-right">
        <li>
          <div class="btn-group" style="margin-left:1px;" role="group">
            <button type="button" class="btn btn-default" onclick="nav.winLeft();">
              <span class="glyphicon glyphicon-chevron-left"></span>
            </button>
            <button type="button" class="btn btn-default" style="width:88px;">Alignment</button>
            <button type="button" class="btn btn-default" onclick="nav.winRight();">
              <span class="glyphicon glyphicon-chevron-right"></span>
            </button>
          </div>
        </li>
