<script>
  nav = new Object();
  nav.winLeft = function(){
    window.moveTo(0, 1);
    window.resizeTo(250, screen.availHeight-1);
  }
  nav.winRight = function(){
    window.moveTo(screen.availWidth-250, 1);
    window.resizeTo(250, screen.availHeight-1);
  }
</script>
<div id="navbar-main">
