<!-- Application Links -->
<style>
  .applink-parts button{
    display:block;
    width:220px;
    margin:0px;
  }
</style>
<script>
  al = new Object();
  al.shell = new ActiveXObject("WScript.Shell");
  al.ieObj = new ActiveXObject("InternetExplorer.Application");
  al.run = function(path){
    this.shell.exec('"'+path+'"');
  }
  al.open = function(url){
    try{
      this.ieObj.visible = true;
    }
    catch(err){
      this.ieObj = new ActiveXObject("InternetExplorer.Application");
      this.ieObj.visible=true;
    }
    finally{
      if(this.ieObj.LocationURL!=''){
        OpenTab = 0x1000;
        this.ieObj.Navigate(url, OpenTab);
      }
      else{
        this.ieObj.Navigate(url);
      }
      title=this.ieObj.name;
      this.shell.appActivate(title);
    }
 }
</script>
